{
  "name": "@samhwang/vite-react-starter-template",
  "version": "1.0.0",
  "description": "Vite + React Starter Template",
  "msw": {
    "workerDirectory": ["public"]
  },
  "type": "module",
  "engines": {
    "node": "^22.0.0",
    "pnpm": "^9.0.0"
  },
  "packageManager": "pnpm@9.15.2",
  "scripts": {
    "prepare": "pnpm run githooks:init",
    "githooks:init": "husky",
    "msw:init": "msw init public --save",
    "dev": "vite",
    "build:static": "rm -rf public/mockServiceWorker.js && vite build",
    "build:function": "vite build --mode page-function",
    "build": "pnpm run build:static && pnpm run build:function",
    "preview": "vite preview",
    "typecheck": "tsc",
    "test": "vitest",
    "test:run": "vitest run",
    "test:watch": "vitest watch",
    "test:CI": "pnpm run test && pnpm run typecheck",
    "test:preview": "vitest-preview",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "lint:fix:unsafe": "biome check --write --unsafe .",
    "format": "biome format --write .",
    "ci": "biome ci .",
    "drizzle:generate": "npx drizzle-kit generate",
    "drizzle:push": "npx drizzle-kit push",
    "drizzle:migrate": "npx drizzle-kit migrate",
    "auth:generate": "npx @better-auth/cli generate --config ./server/auth/auth.ts --output ./server/drizzle/schemas/auth-schema.ts"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^1.32.0",
    "@hono/trpc-server": "^0.3.4",
    "@neondatabase/serverless": "^0.10.4",
    "@tanstack/react-form": "^0.41.0",
    "@tanstack/react-query": "^5.62.11",
    "@tanstack/react-query-devtools": "^5.62.11",
    "@tanstack/react-router": "^1.92.11",
    "@tanstack/router-devtools": "^1.92.11",
    "@tanstack/zod-form-adapter": "^0.41.0",
    "@trpc/client": "11.0.0-rc.682",
    "@trpc/react-query": "11.0.0-rc.682",
    "@trpc/server": "11.0.0-rc.682",
    "better-auth": "^1.1.8",
    "drizzle-orm": "^0.38.4",
    "hono": "^4.6.15",
    "lint-staged": "^15.3.0",
    "pg": "^8.13.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "ws": "^8.18.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@tanstack/router-cli": "^1.92.11",
    "@tanstack/router-vite-plugin": "^1.92.11",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@torchauth/vite-plugin-wrangler-spa": "^2.0.0",
    "@types/node": "^22.10.3",
    "@types/pg": "^8.11.10",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "@types/ws": "^8.5.13",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "drizzle-kit": "^0.30.2",
    "esbuild": "^0.24.2",
    "husky": "^9.1.7",
    "jsdom": "^25.0.1",
    "msw": "^2.7.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vite": "^6.0.6",
    "vitest": "^2.1.8",
    "vitest-preview": "^0.0.1",
    "wrangler": "^3.99.0"
  }
}
