{
  "name": "@samhwang/vite-react-starter-template",
  "version": "1.0.0",
  "description": "Vite + React Starter Template",
  "msw": {
    "workerDirectory": ["public"]
  },
  "type": "module",
  "engines": {
    "node": "^22.0.0",
    "pnpm": "^9.0.0"
  },
  "packageManager": "pnpm@9.14.4",
  "scripts": {
    "prepare": "pnpm run githooks:init",
    "githooks:init": "husky",
    "msw:init": "msw init public --save",
    "dev": "vite",
    "build:static": "vite build",
    "build:function": "vite build --mode page-function",
    "build": "pnpm run build:static && pnpm run build:function",
    "preview": "vite preview",
    "typecheck": "tsc",
    "test": "vitest",
    "test:run": "vitest run",
    "test:watch": "vitest watch",
    "test:CI": "pnpm run test && pnpm run typecheck",
    "test:preview": "vitest-preview",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "lint:fix:unsafe": "biome check --write --unsafe .",
    "format": "biome format --write .",
    "ci": "biome ci .",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "dev:server": "npx tsx server/index.ts"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.1",
    "@tanstack/react-query": "^4.36.1",
    "@tanstack/react-router": "^1.85.5",
    "@tanstack/router-devtools": "^1.85.5",
    "@trpc/client": "^10.45.2",
    "@trpc/react-query": "^10.45.2",
    "@trpc/server": "^10.45.2",
    "better-auth": "^1.1.1",
    "hono": "^4.6.15",
    "lint-staged": "^15.2.10",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@tanstack/router-cli": "^1.85.3",
    "@tanstack/router-vite-plugin": "^1.85.3",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@torchauth/vite-plugin-wrangler-spa": "^2.0.0",
    "@types/node": "^22.10.1",
    "@types/react": "^18.3.13",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "esbuild": "^0.24.2",
    "husky": "^9.1.7",
    "jsdom": "^25.0.1",
    "msw": "^2.6.6",
    "prisma": "^6.1.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vite": "^5.4.11",
    "vitest": "^2.1.8",
    "vitest-preview": "^0.0.1",
    "wrangler": "^3.99.0"
  }
}
