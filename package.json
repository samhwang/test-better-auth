{
  "name": "@samhwang/vite-react-starter-template",
  "version": "1.0.0",
  "description": "Vite + React Starter Template",
  "msw": {
    "workerDirectory": [
      "public"
    ]
  },
  "type": "module",
  "engines": {
    "node": "^22.0.0",
    "pnpm": "^10.0.0"
  },
  "packageManager": "pnpm@10.19.0",
  "scripts": {
    "prepare": "pnpm run githooks:init",
    "githooks:init": "husky",
    "msw:init": "msw init public --save",
    "dev": "vite",
    "build": "pnpm run prisma:generate && pnpm run wrangler:generate && vite build",
    "preview": "vite preview",
    "typecheck": "tsc",
    "test": "vitest",
    "test:run": "vitest run",
    "test:watch": "vitest watch",
    "test:CI": "pnpm run test && pnpm run typecheck",
    "test:preview": "vitest-preview",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "lint:fix:unsafe": "biome check --write --unsafe .",
    "ci": "biome ci .",
    "prisma:generate": "prisma generate",
    "prisma:push": "dotenvx run -- prisma push",
    "prisma:migrate": "dtoenvx run -- prisma migrate dev",
    "auth:generate": "@better-auth/cli generate --config ./server/auth/auth.ts",
    "wrangler:generate": "wrangler types server/generated/worker-configuration.d.ts --env-file .env.sample"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^1.51.0",
    "@orpc/client": "^1.10.0",
    "@orpc/server": "^1.10.0",
    "@orpc/tanstack-query": "^1.10.0",
    "@prisma/adapter-neon": "^6.17.1",
    "@prisma/adapter-pg": "^6.17.1",
    "@prisma/client": "^6.18.0",
    "@tanstack/react-form": "^1.23.7",
    "@tanstack/react-query": "^5.90.5",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@tanstack/react-router": "^1.133.22",
    "@tanstack/react-router-devtools": "^1.133.22",
    "better-auth": "^1.3.28",
    "hono": "^4.10.2",
    "lint-staged": "^16.2.5",
    "pg": "^8.16.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "ws": "^8.18.3",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.6",
    "@cloudflare/vite-plugin": "^1.13.14",
    "@tanstack/router-cli": "^1.133.20",
    "@tanstack/router-vite-plugin": "^1.133.22",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.18.12",
    "@types/pg": "^8.15.5",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/ws": "^8.18.1",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "esbuild": "^0.25.11",
    "husky": "^9.1.7",
    "jsdom": "^27.0.1",
    "msw": "^2.11.6",
    "prisma": "^6.18.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vite": "^6.4.1",
    "vitest": "^3.2.4",
    "vitest-preview": "^0.0.3",
    "wrangler": "^4.44.0"
  }
}
